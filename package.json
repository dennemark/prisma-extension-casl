{
  "name": "prisma-extension-casl",
  "version": "0.1.2",
  "description": "Enforce casl abilities on prisma client ",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/dennemark/prisma-extension-casl.git"
  },
  "scripts": {
    "build": "prisma generate && tsup src/index.ts --format=cjs,esm --dts",
    "watch": "prisma generate && tsup src/index.ts --format=cjs,esm --dts --watch",
    "test": "jest --watch",
    "release": "release-it"
  },
  "keywords": [
    "prisma",
    "@prisma/client",
    "extension"
  ],
  "author": "dennemark",
  "license": "MIT",
  "peerDependencies": {
    "@prisma/client": "latest"
  },
  "devDependencies": {
    "@casl/ability": "^6.7.1",
    "@casl/prisma": "^1.4.1",
    "@prisma/client": "latest",
    "@prisma/generator-helper": "^5.17.0",
    "@release-it/conventional-changelog": "^8.0.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "esbuild": "^0.23.0",
    "jest": "^29.7.0",
    "prisma": "latest",
    "release-it": "^17.6.0",
    "ts-jest": "^29.2.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.1.0",
    "typescript": "^5.5.3"
  }
}
