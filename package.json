{
  "name": "prisma-extension-casl",
  "version": "1.0.0",
  "description": "Enforce casl abilities on prisma client ",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/dennemark/prisma-extension-casl.git"
  },
  "scripts": {
    "build": "prisma generate && tsup src/index.ts --format=cjs,esm --dts",
    "watch": "prisma generate && tsup src/index.ts --format=cjs,esm --dts --watch",
    "test": "jest --watch",
    "release": "release-it"
  },
  "keywords": [
    "prisma",
    "@prisma/client",
    "extension"
  ],
  "author": "dennemark",
  "license": "MIT",
  "peerDependencies": {
    "@prisma/client": "^5.20.0"
  },
  "devDependencies": {
    "@casl/ability": "^6.7.1",
    "@casl/prisma": "^1.5.0",
    "@prisma/client": "^5.20.0",
    "@prisma/generator-helper": "^5.20.0",
    "@release-it/conventional-changelog": "^9.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.7.9",
    "esbuild": "^0.24.0",
    "jest": "^29.7.0",
    "prisma": "5.20.0",
    "release-it": "^17.10.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3"
  }
}
