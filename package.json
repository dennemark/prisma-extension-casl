{
  "name": "prisma-extension-casl",
  "version": "1.3.5",
  "description": "Enforce casl abilities on prisma client ",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/dennemark/prisma-extension-casl.git"
  },
  "scripts": {
    "build": "prisma generate && tsup src/index.ts --format=cjs,esm --dts",
    "watch": "prisma generate && tsup src/index.ts --format=cjs,esm --dts --watch",
    "test": "jest --watch --runInBand",
    "release": "release-it"
  },
  "keywords": [
    "prisma",
    "@prisma/client",
    "extension"
  ],
  "author": "dennemark",
  "license": "MIT",
  "peerDependencies": {
    "@prisma/client": "^6.5.0"
  },
  "devDependencies": {
    "@casl/ability": "^6.7.3",
    "@casl/prisma": "^1.5.1",
    "@prisma/client": "^6.5.0",
    "@prisma/generator-helper": "^6.5.0",
    "@release-it/conventional-changelog": "^10.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.0",
    "esbuild": "^0.25.2",
    "jest": "^29.7.0",
    "prisma": "6.7.0",
    "release-it": "^18.1.2",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2"
  }
}
